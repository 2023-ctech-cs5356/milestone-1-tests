{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  //   "extends": ["next/core-web-vitals", "eslint:recommended"],
  "overrides": [
    {
      "files": ["*.test.?(m|t)js"],
      "env": {
        "jest": true
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "jestPuppeteer": "readonly",
    "page": "readonly",
    "browser": "readonly"
  },
  "rules": {}
}
